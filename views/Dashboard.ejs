<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <link rel="stylesheet" href="/css/style.css" />
  <title>Engine monitoring and control system</title>
  <link rel="shortcut icon" type="image/png" href="/images/Myimages/logo-hh-removebg.png"/>
  <script src="/javascripts/jquery-3.3.1.min.js"></script>
	<script src="/javascripts/clockpicker.js" type="text/javascript"></script>
</head>

<body>
  <div class="wrapper">
    <!-- Sidebar  -->
    <nav id="sidebar">
      <div class="wrap-logo">
        <img id="logocompany" src="/images/Myimages/logo-hh.png" alt="" />
        <div class="expand-sidebar fas fa-angle-double-left" onclick="clickExpand()"></div>
      </div>
      <ul class="list-unstyled components">
        <li>
          <a href="/" class="menu-item active">
            <img src="/images/Myimages/iconDashboard.svg" />
            <p>Dash Board</p>
          </a>
        </li>
        <li>
          <a href="/OeeSupervisor" class="menu-item">
            <img src="/images/Myimages/iconStorage.svg" />
            <p>OEE Supervisor</p>
          </a>
        </li>
        <li>
          <a href="/DataAnalytics" class="menu-item">
            <img src="/images/Myimages/iconChart.svg" />
            <p>Data Analytics</p>
          </a>
        </li>
        <li>
          <a href="/AugmentedReality" class="menu-item">
            <img src="/images/Myimages/iconCulture.svg" />
            <p>Augmented Reality</p>
          </a>
        </li>
      </ul>
    </nav>
    <!-- Page Content  -->
    <div id="content" >
      <div class="overlay"></div>
      <nav class="navbar navbar-expand-lg navbar-light">
        <div class="container-top">
          <!-- Button hide navbar -->
          <button class="hide-nav-bar" type="button" data-toggle="collapse"
            data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
            aria-label="Toggle navigation" onclick="clickHideSide()">
            <i class="fas fa-align-justify hide-button"></i>
            <div class="close-button">X</div>
          </button>
          <div class="heading">
            Engine monitoring & control system
          </div>
          <img class="img-walk" src="/images/Myimages/worker.gif"/>
        </div>
      </nav>
      <div class="main-container">
        <div class="col">
          <div class="card-dasboard mb-3">
            <div class="card-header">
              <div class="row">
                <h5>Speed</h5>
                <div class="switch-button">
                  <input type="checkbox" id="enableSpeed" data-toggle="toggle" data-on=" " data-off=" "
                    data-onstyle="success" data-offstyle="danger" data-width="100" />
                </div>
              </div>
            </div>
            <div class="card-body">
              <div class="wrap-gauge">
                <div class="led" id="ledStatus">
                  <span id="textStatus"></span>
                </div>
                <div class="GaugeMeter" id="GaugeMeter_1" data-min="0" data-total="1500" data-text="null"
                  data-text_size="0.16" data-prepend="null" data-append=" rpm"
                  data-style="Arch" data-color="null" data-back="null" data-theme="null"
                  data-animate_gauge_colors="flase" data-animate_text_colors="false" data-label="null"
                  data-label_color="null" data-stripe="null" data-showvalue="true"></div>
              </div>

              <div class="wrap-control row">
                <button type="button" class="btn btn-success col" id="motorStart" disabled>
                  Start
                </button>
                <button type="button" class="btn btn-danger col" id="motorStop" disabled>
                  Stop
                </button>
              </div>
              <div class="wrap-control row">
                <input type="number" class="form-control col-9" id="motorSetpointbox" placeholder="Speed" min="0"
                  max="1500" value="" disabled />
                <button type="button" class="btn btn-warning col" id="motorSetpoint" disabled>
                  Set
                </button>
              </div>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card-dasboard mb-3">
            <div class="card-header">
              <div class="row">
                <h5>Voltage</h5>
                <div class="switch-button">
                  <input type="checkbox" id="enableVoltage" data-toggle="toggle" data-on=" " data-off=" "
                    data-onstyle="success" data-offstyle="danger" data-width="100" />
                </div>
              </div>
            </div>
            <div class="card-body">
              <div class="wrap-chart">
                <canvas id="voltChart"></canvas>
              </div>
              <div class="set-limit row">
                <input type="number" class="form-control col-9" id="voltageSetpointbox" placeholder="High limit" />
                <button type="button" class="btn btn-warning col" id="voltageSetpoint">
                  Set
                </button>
              </div>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card-dasboard mb-3">
            <div class="card-header">
              <div class="row">
                <h5>Vibration</h5>
                <div class="switch-button">
                  <input type="checkbox" id="enableVibration" data-toggle="toggle" data-on=" " data-off=" "
                    data-onstyle="success" data-offstyle="danger" data-width="100" />
                </div>
              </div>
            </div>
            <div class="card-body">
              <div class="wrap-chart">
                <canvas id="vibrationChart"></canvas>
              </div>
              <div class="set-limit row">
                <input type="number" class="form-control col-9" id="vibrationSetpointbox" placeholder="High limit" />
                <button type="button" class="btn btn-warning col" id="vibrationSetpoint">
                  Set
                </button>
              </div>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card-dasboard mb-3">
            <div class="card-header">
              <div class="row">
                <h5>Time Schedule</h5>
                <div class="switch-button">
                  <input type="checkbox" id="enableTimeSchedule" data-toggle="toggle" data-on=" " data-off=" "
                    data-onstyle="success" data-offstyle="danger" data-width="100" />
                </div>
              </div>
            </div>
            <div class="card-body">
              <div class="wrap-time row">
                <div class="timeRunning">Time server</div>
                <div id="clock">?:?</div>
              </div>
              <div class="wrap-time row">
                <div class="timeRunning">
                  Start time
                </div>
                <div>
                  <div class="input-group clockpicker">
                    <input type="text" class="form-control pick-input" id="startTimeBox" value="18:00" style="text-align: center"/>
                  </div>
                  <script type="text/javascript">
                    $(".clockpicker").clockpicker({
                      placement: "top",
                      align: "right",
                      donetext: "Done",
                    });
                  </script>
                </div>
              </div>
              <div class="wrap-time row">
                <div class="timeRunning">
                  Stop time
                </div>
                <div>
                  <div class="input-group clockpicker">
                    <input type="text" class="form-control pick-input" id="stopTimeBox" value="18:00" style="text-align: center"/>
                  </div>
                  <script type="text/javascript">
                    $(".clockpicker").clockpicker({
                      placement: "top",
                      align: "right",
                      donetext: "Done",
                    });
                  </script>
                </div>
              </div>
              <div class="wrap-time row">
                <div class="timeRunning">Day</div>
                <div class="weekDays-selector">
                  <input type="checkbox" id="weekday-mon" class="weekday" />
                  <label for="weekday-mon">Mo</label>
                  <input type="checkbox" id="weekday-tue" class="weekday" />
                  <label for="weekday-tue">Tu</label>
                  <input type="checkbox" id="weekday-wed" class="weekday" />
                  <label for="weekday-wed">We</label>
                  <input type="checkbox" id="weekday-thu" class="weekday" />
                  <label for="weekday-thu">Th</label>
                  <input type="checkbox" id="weekday-fri" class="weekday" />
                  <label for="weekday-fri">Fr</label>
                  <input type="checkbox" id="weekday-sat" class="weekday" />
                  <label for="weekday-sat">Sa</label>
                  <input type="checkbox" id="weekday-sun" class="weekday" />
                  <label for="weekday-sun">Su</label>
                </div>
              </div>
              <div class="btn-submit">
                <button type="submit" id="submitTime" class="btn btn-primary">
                  Submit
                </button>
              </div>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card-dasboard mb-3">
            <div class="card-header">
              <div class="row">
                <h5>Current</h5>
                <div class="switch-button">
                  <input type="checkbox" id="enableCurrent" data-toggle="toggle" data-on=" " data-off=" "
                    data-onstyle="success" data-offstyle="danger" data-width="100" />
                </div>
              </div>
            </div>
            <div class="card-body">
              <div class="wrap-chart">
                <canvas id="currentChart"></canvas>
              </div>
              <div class="set-limit row">
                <input type="number" class="form-control col-9" id="currentSetpointbox" placeholder="High limit" />
                <button type="button" class="btn btn-warning col" id="currentSetpoint">
                  Set
                </button>
              </div>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card-dasboard mb-3">
            <div class="card-header">
              <div class="row">
                <h5>Temperature</h5>
                <div class="switch-button">
                  <input type="checkbox" id="enableTemperature" data-toggle="toggle" data-on=" " data-off=" "
                    data-onstyle="success" data-offstyle="danger" data-width="100" />
                </div>
              </div>
            </div>
            <div class="card-body">
              <div class="wrap-chart">
                <canvas id="temperatureChart"></canvas>
              </div>
              <div class="set-limit row">
                <input type="number" class="form-control col-9" id="temperatureSetpointbox" placeholder="High limit" />
                <button type="button" class="btn btn-warning col" id="temperatureSetpoint">
                  Set
                </button>
              </div>
            </div>
          </div>
        </div>

      </div>
    </div>
  </div>

  <!-- Font Awesome JS -->
  <script src="/javascripts/solid.js"></script>
  <script src="/javascripts/fontawesome.js"></script>

  <!-- Include all compiled plugins (below), or include individual files as needed -->
  <script src="/javascripts/popper.min.js"></script>
  <script src="/javascripts/bootstrap-4.2.1.js"></script>
  <script src="/javascripts/bootstrap4-toggle.min.js"></script>

  <script src="/javascripts/Myjavascripts/speed-gauge-meter.js"></script>

  <!-- Chart -->
  <script src="/javascripts/moment.min.js"></script>
  <script src="/javascripts/chart@2.9.0.js"></script>
  <script src="/javascripts/chartjs-plugin-streaming@1.8.0.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/hammerjs@2.0.8"></script>
  <script src="path/to/chartjs-plugin-zoom/dist/chartjs-plugin-zoom.min.js"></script>

  <!-- Annotation-Range -->
  <script src="/javascripts/chartjs-plugin-annotation.js"></script>
  <script src="/javascripts/chartjs-plugin-annotation.min.js"></script>

  <!-- Time-Digital -->
  <script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
  <script defer src="/javascripts/Myjavascripts/time-digital.js"></script>

  <!-- Time-Check-Box -->

  <!-- SocketIO -->
  <script src="/socket.io/socket.io.js"></script>
  <!-- Speed -->
  <script src="/javascripts/Myjavascripts/Dashboard.js"></script>
  <script src="/javascripts/Myjavascripts/sidebar.js"></script>

</body>

</html>